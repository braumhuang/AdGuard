https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/BiLi/main.qxf, tag=哔哩哔哩, force-policy=哔哩哔哩, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/WeChat/main.qxf, tag=微信直连, force-policy=direct, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/Voice/main.qxf, tag=Google Voice, force-policy=美国节点, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/OpenAI/main.qxf, tag=OpenAi, force-policy=美国节点, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/China/main.qxf, tag=国内网站, force-policy=direct, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/Streaming/main.qxf, tag=国际媒体, force-policy=国际媒体, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/Proxy/main.qxf, tag=全球加速, force-policy=全球加速, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/Unbreak/main.qxf, tag=规则修正, force-policy=direct, update-interval=-1, opt-parser=true, enabled=true
https://raw.githubusercontent.com/braumhuang/AdGuard/master/Filter/AdBlock/main.qxf, tag=广告拦截, force-policy=reject, update-interval=-1, opt-parser=true, enabled=true


[filter_local]
host-suffix, dmm.co.jp, 日本节点
host-suffix, local, direct
ip-cidr, 192.168.0.0/16, direct
ip-cidr, 10.0.0.0/8, direct
ip-cidr, 172.16.0.0/12, direct
ip-cidr, 127.0.0.0/8, direct
ip-cidr, 100.64.0.0/10, direct
ip-cidr, 224.0.0.0/4, direct
ip-cidr, 183.240.197.130/32, direct
ip6-cidr, fe80::/10, direct
# GeoIP（Geolocation IP）是一种技术，用于确定特定IP地址的地理位置信息，如国家、城市、经纬度等。
geoip, cn, direct
# 不在上述规则中(远程以及本地)的剩余请求，将走final 指定的节点/策略
final, 兜底分流


[http_backend]
# BoxJS已不需要此配置
;https://raw.githubusercontent.com/braumhuang/AdGuard/master/Rewrite/BoxJS/Script/box.js, host=boxjs.com, tag=BoxJS, path=^/, enabled=true


[mitm]
# 本地主机名
hostname = -www.google.com, -*.amemv.com, -*.snssdk.com, boxjs.net, boxjs.com,
# 证书
skip_validating_cert = true